    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "LND watcher",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/src/lndWatcher.ts",
        "preLaunchTask": "npm: build",
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "outputCapture": "std",
      },
      {
        "type": "node",
        "request": "launch",
        "name": "worker",
        "skipFiles": ["<node_internals>/**"],
        "program": "${workspaceFolder}/src/worker.ts",
        "preLaunchTask": "npm: build",
        "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        "outputCapture": "std",
      },
    ]
  }